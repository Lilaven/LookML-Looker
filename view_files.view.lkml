      WHEN svc_scp_cd = 'LEA' AND n1st_pol_cd LIKE 'MX%' THEN 'N-DOM'
      WHEN svc_scp_cd = 'LEA' THEN 'DOM'
      WHEN svc_scp_cd = 'IAA' AND right(t_vvd_cd,1) IN ('S','W') THEN 'DOM'
