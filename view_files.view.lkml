              THEN CAST( (EXTRACT(YEAR  FROM date_trunc(current_date(), month)   )*100) + 4 AS STRING )
              ELSE CAST( (EXTRACT(YEAR  FROM date_trunc(current_date(), month)-1 )*100) + 4 AS STRING )
         END
    ;;
    }

