                                                      #sql: ${cm_amt} ;;
                                                      #waitlisted_yesno|jack-2022-4-10|before>>

                                                      #waitlisted_yesno|jack-2022-4-10|after<<
                                                      sql:
                                                              {% if include_waitlisted_bkg._parameter_value == "'Yes'" %}
                                                                ${cm_amt}
                                                              {% else %}
                                                                CASE ${bkg_sts_cd} WHEN 'W' THEN NULL ELSE ${cm_amt} END
                                                              {% endif %};;
                                                            #waitlisted_yesno|jack-2022-4-10|after>>

