      , CAST( (CASE WHEN of_svc_scp.Service_Scope_Group_Code = 'TP'
      THEN cm_amt2   - (IFNULL(othercost.VIP_AMT_PER_TEU_USD,0) * (case when substr(cntr_tpsz,1,1)='Q' then 0 else bkg_teu end) )
