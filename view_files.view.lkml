    #  left join ${of_customer_group.SQL_TABLE_NAME} g on (a.cust_grp_id = g.cust_grp_id and g.delt_flg = 'N')
    #  left join (select b.cust_cd, w.fiscal_year, sum(bkg_teu) teu
