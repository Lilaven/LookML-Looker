      #  CASE
      #    WHEN {% parameter timeframe_picker %} = 'Week'  THEN ${of_sales_week_master.sales_week}
      #    WHEN {% parameter timeframe_picker %} = 'Month' THEN ${of_sales_week_master.sales_month}
