      drill_fields: [of_booking.basic_details*, total_cancelled_teu]
    }

    measure: percent_cancellation {
      description: "Cancelled TEU / Total Booking TEU"
      type: number
      sql: ${total_cancelled_teu} / NULLIF(${total_booking_teu}, 0) ;;
