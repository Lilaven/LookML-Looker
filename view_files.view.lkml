    datatype: date
    sql: ${TABLE}.RCT_UPD_DT;;

  }
  dimension: adj_upd_dt {
    type: date_time
    datatype: date
    sql:${TABLE}.ADJ_UPD_DT;;

  }
  dimension: bal_upd_dt {
    type: date_time
    datatype: date
    sql: ${TABLE}.BAL_UPD_DT;;

