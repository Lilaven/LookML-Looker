    dimension: 1_complete_mth_ago {
      type:  string
      sql: CAST(
         EXTRACT(YEAR  FROM DATE_SUB( date_trunc(current_date(), month), INTERVAL 1 MONTH) ) * 100 +
         EXTRACT(MONTH FROM DATE_SUB( date_trunc(current_date(), month), INTERVAL 1 MONTH) )
