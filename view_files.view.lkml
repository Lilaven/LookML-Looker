  dimension: fiscal_year {
    label: "Fiscal Year"
    type: string
    sql: ${TABLE}.fiscal_year ;;
  }

  dimension: teu {
    label: "FY. TEU"
    type: number
    sql: ${TABLE}.teu ;;
  }

  set: detail {
    fields: [cust_cd, cust_grp_id, key_acct_tp_cd, teu]
  }
}

