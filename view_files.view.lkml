      coalesce(of_svc_contract.cust_cd, of_actuals.cnt_cust_cd) = v_customer_opus_n_prospect.cust_cd AND  coalesce (of_svc_contract.cust_cd,of_actuals.cnt_cust_cd) IS NOT NULL
