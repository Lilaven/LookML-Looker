              {% elsif timeframe_picker._parameter_value == "'Quarter'"         %} ${calendar_quarter_new_fmt}
              {% elsif timeframe_picker._parameter_value == "'Fiscal Quarter'"  %} ${fiscal_quarter_new_fmt}
