      WHEN   o.OFFICE_CODE = 'CANBB' then 'HK & CNS'
      WHEN   o.OFFICE_CODE = 'SWABA' then 'HK & CNS'
      WHEN   o.OFFICE_CODE = 'SZPBB' then 'HK & CNS'
